spring.mvc.pathmatch.matching-strategy=ant-path-matcher

#TOMCAT
server.port = 8080

# CONEX√ÉO COM O BANCO DE DADOS MYSQL
spring.datasource.url=jdbc:mysql://localhost/app-financeiro-db?useTimezone=true&serverTimezone=UTC
